{
  "name": "my-react",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "babel-eslint": "^8.0.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.12.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "react-scripts": "^1.0.17",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "history": "^4.7.2",
    "re-base": "^3.2.0",
    "react": "^16.1.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-router": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "eject": "react-scripts eject"
  },
  "description": "My React app. Hope it's smooth sailing.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxwellN/my-react.git"
  },
  "keywords": [
    "a",
    "react",
    "app"
  ],
  "author": "maxwell narten",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaxwellN/my-react/issues"
  },
  "homepage": "https://github.com/MaxwellN/my-react#readme"
}
